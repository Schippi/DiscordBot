<!DOCTYPE html>
<html>
    <head>
        <title>Beatsaber</title>
        <link rel="stylesheet" href="/bscss/dark-theme.css"/>
        <link rel="icon" type="image/png" href="/bscss/favicon.png"/>
        <meta property="og:image" content="/bscss/favicon.png"/>
        <meta property="og:title" content="Beatsaber"/>
        <meta property="og:type" content="website"/>
        <meta property="og:description" content="BS Stuff"/>
        <meta name="Description" content="BS Stuff"/>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
        <meta name="viewport" content="width=device-width,initial-scale=1"/>
        <meta name="theme-color" content="#171a21"/>
        <meta property="og:site" content="Beatsaber"/>
    </head>
<body>
<input type="text" id="myInput" onkeyup="filterGames()" placeholder="Search for names.." title="Type in a name"><br/>
    <script>
    function filterGames() {
        var input, filter, ul, li, a, i, txtValue;
        input = document.getElementById("myInput");
        filter = input.value.toUpperCase();

        const collection = document.getElementsByClassName("tooltip");
        for (i = 0; i < collection.length; i++) {
            myimg = collection[i].getElementsByClassName("mytxt")[0];
            imgtxt = myimg.textContent.replace('<br>','')
            if (imgtxt.toUpperCase().indexOf(filter) > -1) {
                collection[i].style.display = "";
            } else {
                collection[i].style.display = "none";
            }
        }

    }
    </script>